{% trans_default_domain 'admin' %}
{% extends easyadmin_config('design.templates.layout') %}

{% block page_title %}{{ 'image.list.title'|trans }}{% endblock %}

{% block content %}
    <section id="main" class="admin-content kt-container kt-container--fluid kt-grid__item kt-grid__item--fluid">
        <div id="input-image-manager">
            <input-image-manager
                current-year="{{ currentYear }}"
                :folders="{{ folders }}"
                :images="{{ images }}"/>
        </div>
    </section>
{% endblock %}
